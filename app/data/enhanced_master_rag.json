  "scan_commands": {
    "nuclei": {
      "basic": "nuclei -u {target} -json -o {output}",
      "with_templates": "nuclei -u {target} -t nuclei-templates/ -json -o {output}",
      "severity_filter": "nuclei -u {target} -severity critical,high -json -o {output}",
      "with_auth": "nuclei -u {target} -H 'Cookie: {cookies}' -json -o {output}"
    },
    "ffuf": {
      "directory": "ffuf -u {target}/FUZZ -w {wordlist} -mc 200,301,302 -o {output}",
      "parameter": "ffuf -u '{target}?FUZZ=1' -w {wordlist} -mc 200 -o {output}",
      "with_auth": "ffuf -u {target}/FUZZ -w {wordlist} -H 'Cookie: {cookies}' -mc 200 -o {output}"
    },
    "gospider": {
      "basic": "gospider -s {target} -t 10 -o {output}",
      "with_js": "gospider -s {target} -t 10 --js -o {output}",
      "with_auth": "gospider -s {target} -t 10 --cookie '{cookies}' -o {output}"
    },
    "sqlmap": {
      "basic": "sqlmap -u '{target}' --batch --threads=3",
      "with_auth": "sqlmap -u '{target}' --cookie='{cookies}' --batch --threads=3",
      "comprehensive": "sqlmap -u '{target}' --batch --threads=3 --risk=2 --level=2"
    },
    "dalfox": {
      "basic": "dalfox file {urls_file} --basic-payloads -o {output}",
      "with_auth": "dalfox file {urls_file} --cookie '{cookies}' --basic-payloads -o {output}"
    }
  },

